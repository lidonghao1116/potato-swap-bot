# 网络配置
RPC_URL=https://rpc.xlayer.tech
CHAIN_ID=196
# 主钱包私钥 (仅用于distribute.ts分发代币)
MAIN_WALLET_PRIVATE_KEY=0x...
# 子钱包配置
NUMBER_OF_WALLETS=2
# 每个子钱包需要的OKB数量
OKB_PER_WALLET=0.1
# 每个子钱包需要的USDT数量
USDT_PER_WALLET=13
# 子钱包私钥 (用逗号分隔，数量应与NUMBER_OF_WALLETS匹配)
SUB_WALLET_PRIVATE_KEYS=0x...,0x...

# 合约地址
# 注意：OKB是X Layer原生代币，不需要合约地址
USDT_CONTRACT=0x1e4a5963abfd975d8c9021ce480b42188849d41d
POTATO_SWAP_ROUTER=0x881fb2f98c13d521009464e7d1cbf16e1b394e8e
